<?php
$multiplier = 1;
$value = 1;
    
fscanf (STDIN, "%f", $a);
fscanf (STDIN, "%f", $b);

if ($a>=$b){
    $value1 = $a;
    $value2 = $b;
}else{
    $value1 = $b;
    $value2 = $a;
}if ($value1*$multiplier % $value2 == 0) {
    $value = $value1*$multiplier;
    echo $value . "\n";
}else{
    while($value1*$multiplier % $value2 <> 0){
        $multiplier += 1;
        $value = $value1*$multiplier;
        if ($value1*$multiplier % $value2 == 0){
        echo $value . "\n";
        }
    }
} 
?>