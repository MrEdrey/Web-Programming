<?php
$result = 0;
$negative_a = false;
$negative_b = false;

fscanf (STDIN, "%f", $a);
fscanf (STDIN, "%f", $b);

if ($a < 0){
    $a = -$a;
    $negative_a = true;
}if ($b < 0){
    $b = -$b;
    $negative_b = true;
}while ($a >= $b){
    $a = $a - $b;
    $result += 1;
}if ($negative_a && $negative_b){
    echo $result . "\n", $a;
}elseif ($negative_a xor $negative_b){
    echo (-$result) . "\n", $a;
}else{
    echo $result . "\n", $a;
}
?>